{
    "Checkbox with label": {
        "prefix": "tca-check",
        "body": [
            "'${1:fieldname}' => [",
            "   'exclude' => 1,",
            "   'label' => '${2:Label}',",
            "   'config' => [",
            "      'type' => 'check',",
            "      'items' => [",
            "         ['${2:Item Label}', '${3:Value}'],",
            "      ],",
            "   ]",
            "],"
        ],
        "description": "Checkbox with label"
    },
